<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.studybnb">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_foreground"
        android:label="@string/app_name"
        android:noHistory="true"
        android:roundIcon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/Theme.StudyBNB">
        <activity
            android:name=".SubjectSelectActivity"
            android:exported="true" />

        <meta-data
            android:name="com.google.firebase.ml.vision.DEPENDENCIES"
            android:value="ocr" />

        <activity
            android:name=".toeic.NoteViewActivity"
            android:exported="true" />
        <activity
            android:name=".SubjectActivity"
            android:exported="false" />
        <activity
            android:name=".InfoActivity"
            android:exported="true" />
        <activity
            android:name=".toeic.NoteListActivity"
            android:exported="true" />
        <activity
            android:name=".toeic.NoteWriteActivity"
            android:exported="true" />
        <activity
            android:name=".SettingActivity"
            android:exported="true" />
        <activity
            android:name=".LoginActivity"
            android:exported="true" />
        <activity
            android:name=".MainActivity"
            android:exported="true" />
        <activity
            android:name=".cs.CsNoteListActivity"
            android:exported="true" />
        <activity
            android:name=".cs.CsNoteWriteActivity"
            android:exported="true" />
        <activity
            android:name=".cs.CsNoteViewActivity"
            android:exported="true" />
        <activity
            android:name=".history.HistoryNoteViewActivity"
            android:exported="true" />
        <activity
            android:name=".history.HistoryNoteListActivity"
            android:exported="true" />
        <activity
            android:name=".history.HistoryNoteWriteActivity"
            android:exported="true" />
        <activity
            android:name=".toeic.TimerActivity"
            android:exported="true" />
        <activity
            android:name=".SplashActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".cs.CsTimerActivity"
            android:exported="true" />
        <activity
            android:name=".history.HistoryTimerActivity"
            android:exported="true" />
    </application>

</manifest>